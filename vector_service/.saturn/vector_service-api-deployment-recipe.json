{
  "name": "vector_service",
  "image": {
    "name": "saturn-python",
    "version": "2022.04.01",
    "owner": "saturncloud"
  },
  "description": "Vector Service API deployment ",
  "environment_variables": {},
  "working_directory": "/home/jovyan/workspace/redisventures-hackunamadata/vector_service",
  "extra_packages": {
    "pip": {
      "install": "-r requirements.txt"
    }
  },
  "start_script": "export PYTHONPATH=.",
  "git_repositories": [
    {
      "url": "git@github.com:artefactory/redisventures-hackunamadata.git",
      "path": "/home/jovyan/workspace/redisventures-hackunamadata",
      "public": false,
      "on_restart": "reclone",
      "reference": "feat/vector_service",
      "reference_type": "branch"
    }
  ],
  "secrets": [
    {
      "location": "REDIS_PASSWORD",
      "type": "environment_variable",
      "description": "Redis password",
      "owner": "vectorization",
      "name": "REDIS_PASSWORD"
    },
    {
      "location": "REDIS_HOST",
      "type": "environment_variable",
      "description": "Redis host",
      "owner": "vectorization",
      "name": "REDIS_HOST"
    },
    {
      "location": "REDIS_PORT",
      "type": "environment_variable",
      "description": "Redis port",
      "owner": "vectorization",
      "name": "REDIS_PORT"
    },
    {
      "location": "REDIS_DB",
      "type": "environment_variable",
      "description": "Redis DB",
      "owner": "vectorization",
      "name": "REDIS_DB"
    }
  ],
  "deployment": {
    "instance_type": "medium",
    "command": "uvicorn api.main:app --reload --host 0.0.0.0 --port 8000",
    "instance_count": 1,
    "public": false,
    "start_ssh": false,
    "subdomain": "vectorservice"
  },
  "owner": "vectorization",
  "visibility": "org",
  "schema_version": "2022.08.01"
}